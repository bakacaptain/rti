package core;

import java.util.concurrent.BlockingQueue;

/// Call start on an instance of DataSubscriber to call the run() function.
public class DataSubscriber<T> extends Thread {

	private BlockingQueue<T> queue;
	
	public DataSubscriber(BlockingQueue<T> queue){
		this.queue = queue;
	}
	
	public void run(){
		try{
			while(true){
				
			}
		}catch(Exception ex){
			ex.printStackTrace();
		}
		
	}
}
